    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.14.1",
    "@fontsource/inter": "^5.2.6",
    "@mui/icons-material": "^7.1.2",
    "@mui/joy": "^5.0.0-beta.52",
    "@mui/material": "^5.16.7",
    "@types/lodash.isequal": "^4.5.8",
    "@types/node": "^24.0.8",
    "@types/pako": "^2.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-modal": "^3.16.3",
    "@types/uuid": "^10.0.0",
    "express": "^5.1.0",
    "google-protobuf": "^3.21.4",
    "lodash.isequal": "^4.5.0",
    "long": "^5.3.2",
    "pako": "^2.1.0",
    "pocketbase": "^0.26.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-is": "^19.1.0",
    "react-modal": "^3.16.1",
    "ts-proto": "^2.7.5"
  },
  "optionalDependencies": {
    "mdns": "^2.7.2"
  },
  "scripts": {
    "start": "yarn run generate-api && vite",
    "build": "yarn run generate-api && tsc && vite build",
    "serve": "vite preview",
    "discovery": "node ./src/server.js",
    "generate-api": "protoc --proto_path=../api --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/api --ts_proto_opt=esModuleInterop=true,forceLong=long,useOptionals=messages,env=browser ../api/bloop.proto",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/events": "^3.0.3",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/ui": "^2.1.8",
    "jsdom": "^26.1.0",
    "typescript": "^5.9.2",
    "vite": "^6.2.4",
    "vite-plugin-top-level-await": "^1.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  }
